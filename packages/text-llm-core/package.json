{
  "name": "@mask-cut/text-llm-core",
  "version": "0.1.0",
  "description": "Core masking utilities for Mask-Cut LLM integrations.",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
